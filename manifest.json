{
  "manifest_version": 2,

  "name": "Url Editor",
  "description": "This extension lets you edit the url GET parameters easily",
  "version": "1.0",

	"short_name": "Urledit",

	"permissions": ["tabs"],
  "browser_action": {
    "default_icon": "icon128.png",
    "default_popup": "popup.html"
  },
	"icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
	"background": {
    "scripts": ["popup.js"]
  }
}
